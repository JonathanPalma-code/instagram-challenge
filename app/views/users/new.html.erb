<h1>Sign Up</h1>
<%= form_for @user do |f|%>
  <%= f.label :username%><br>
  <%= f.text_field :username, name: 'username'%><br>
  <%= f.label :email%><br>
  <%= f.text_field :email, name: 'email'%><br>
  <%= f.label :password%><br>
  <%= f.password_field :password, name: 'password'%><br>
  <%= f.label :password_confirmation%><br>
  <%= f.password_field :password_confirmation, name: 'password_confirmation'%><br>
  <%= f.submit :Submit%>
<%end%>